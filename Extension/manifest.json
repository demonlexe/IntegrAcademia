{
    "manifest_version": 3,
    "name": "Integra-Academia",
    "description": "An integrated tool for class registration.",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "./assets/hello_extensions.png"
    },
    "content_scripts": [
    {
      "matches": ["https://utdallas.collegescheduler.com/terms/*/courses/*"],
      "css": ["content-script/styles.css"],
      "js": ["content-script/index.js"]
    }],
    "permissions": [
      "storage"
    ]
  }